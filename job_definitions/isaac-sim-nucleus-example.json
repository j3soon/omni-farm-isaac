{
  "job_type": "base",
  "command": "bash",
  "args": ["-c"],
  "job_spec_path": "null",
  "capacity_requirements": {
    "resource_limits": {
      "nvidia.com/gpu": 1
    },
    "env_from": [
      {
        "secretRef": {
          "name": "nucleus-secret"
        }
      }
    ],
    "active_deadline_seconds": 864000,
    "affinity": {
      "podAffinity": {
        "preferredDuringSchedulingIgnoredDuringExecution": [
          {
            "weight": 1,
            "podAffinityTerm": {
              "labelSelector": {
                "matchExpressions": [
                  {
                    "key": "ov-farm",
                    "operator": "Exists"
                  }
                ]
              },
              "topologyKey": "kubernetes.io/hostname"
            }
          }
        ]
      }
    }
  },
  "container": "j3soon/omni-farm-isaac-sim:4.1.0",
  "allowed_args": {
    "args": {
      "arg": "",
      "default": "echo hi",
      "separator": ""
    }
  }
}
