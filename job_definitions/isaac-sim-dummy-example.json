{
  "job_type": "base",
  "command": "./python.sh",
  "args": ["-u"],
  "job_spec_path": "null",
  "capacity_requirements": {
    "resource_limits": {
      "nvidia.com/gpu": 1
    },
    "env": [
      {
        "name": "NODE_NAME",
        "valueFrom": {
          "fieldRef": {
            "fieldPath": "spec.nodeName"
          }
        }
      }
    ],
    "active_deadline_seconds": 864000,
    "affinity": {
      "podAffinity": {
        "preferredDuringSchedulingIgnoredDuringExecution": [
          {
            "weight": 1,
            "podAffinityTerm": {
              "labelSelector": {
                "matchExpressions": [
                  {
                    "key": "ov-farm",
                    "operator": "Exists"
                  }
                ]
              },
              "topologyKey": "kubernetes.io/hostname"
            }
          }
        ]
      }
    },
    "dnsConfig": {
      "nameservers": [
        "8.8.8.8",
        "8.8.4.4"
      ]
    },
    "hostIPC": true
  },
  "container": "nvcr.io/nvidia/isaac-sim:4.5.0",
  "allowed_args": {
    "args": {
      "arg": "",
      "default": "./standalone_examples/api/omni.isaac.core/time_stepping.py",
      "separator": ""
    }
  }
}
